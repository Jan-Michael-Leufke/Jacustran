@page "/counterFromSecClient"

<PageTitle>Counter from Client 2</PageTitle>

<h1>Counter from client 2</h1>

<p role="status">Current count: @currentCount</p>

<button class="btn btn-primary" @onclick="IncrementCount">Click me on client 2</button>

@code {
    private int currentCount = 0;

    private void IncrementCount()
    {
        currentCount++;
    }
}
